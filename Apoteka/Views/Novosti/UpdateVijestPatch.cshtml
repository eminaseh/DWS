@model Vijest
@{  ViewBag.Title = "Uredi vijest PATCH request";}

<a asp-action="Index" class="btn mt-5 mb-5 btn-primary">Nazad</a>

<form method="post">
    <div class="form-group">
        <label asp-for="Naslov"></label>
        <input class="form-control" asp-for="Naslov" />
    </div>
    <div class="form-group">
        <label asp-for="Opis"></label>
        <input class="form-control" asp-for="Opis" />
    </div>
    <div class="text-center panel-body">
        <button type="submit" class="btn  btn-primary">Potvrdi</button>
    </div>
</form>